{
  "name": "anime-search",
  "version": "2.0.1",
  "description": "An anime search engine that scrapes data from famous anime sites to find your favorite anime.",
  "maintainers": [
    "MDxWARRIORxOP <mohammedsiddiqui2009@gmail.com>"
  ],
  "scripts": {
    "build": "bash ./build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.mjs.json",
    "prepare": "yarn build",
    "prepublishOnly": "yarn lint",
    "version": "yarn format",
    "test": "jest --coverage",
    "test:coveralls": "jest --coverage && coveralls < coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MDxWARRIORxOP/anime-search.git"
  },
  "keywords": [
    "anime",
    "anime-search",
    "anime-search-engine",
    "search"
  ],
  "author": "MDxWARRIORxOP <mohammedsiddiqui2009@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MDxWARRIORxOP/anime-search/issues"
  },
  "homepage": "https://github.com/MDxWARRIORxOP/anime-search#readme",
  "main": "lib/cjs/index.js",
  "types": "lib/types/index.d.ts",
  "module": "lib/mjs/index.js",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/mjs/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "packageManager": "yarn@3.3.1",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "closest-match": "^1.3.3",
    "node-fetch": "2.6.7"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/node-fetch": "^2.6.2",
    "coveralls": "^3.1.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.4"
  }
}
