{
  "name": "anime-search",
  "version": "2.2.1",
  "description": "An anime search engine that scrapes data from famous anime sites to find your favorite anime.",
  "maintainers": [
    "MDxWARRIORxOP <mohammedsiddiqui2009@gmail.com>"
  ],
  "scripts": {
    "build": "bash ./build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint -c .eslintrc.js --ext .ts src/",
    "test": "jest --coverage",
    "typedoc": "rm -rf docs && typedoc --plugin typedoc-plugin-rename-defaults  --plugin typedoc-plugin-extras --plugin @mxssfd/typedoc-theme --entryPointStrategy expand src/ --out docs  --tsconfig tsconfg.json --theme my-theme --name Anime-Search-Docs --customTitle Anime-Search-Docs --favicon https://cdn.discordapp.com/attachments/894260496725835776/1063899048588886207/AS_Logo.png --footerLastModified true",
    "prepublish": "yarn build",
    "prepublishOnly": "yarn lint && yarn test",
    "version": "yarn format"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MDxWARRIORxOP/anime-search.git"
  },
  "keywords": [
    "anime",
    "anime-search",
    "anime-search-engine",
    "search"
  ],
  "author": "MDxWARRIORxOP <mohammedsiddiqui2009@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MDxWARRIORxOP/anime-search/issues"
  },
  "homepage": "https://github.com/MDxWARRIORxOP/anime-search#readme",
  "main": "lib/cjs/index.js",
  "types": "lib/types/index.d.ts",
  "module": "lib/mjs/index.js",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/mjs/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "packageManager": "yarn@3.3.1",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "closest-match": "^1.3.3",
    "eslint-plugin-tsdoc": "^0.2.17",
    "node-fetch": "2.6.7"
  },
  "devDependencies": {
    "@mxssfd/typedoc-theme": "^1.0.3",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.23.24",
    "typedoc-plugin-extras": "^2.3.2",
    "typedoc-plugin-google-ads": "^1.1.0",
    "typedoc-plugin-rename-defaults": "^0.6.4",
    "typescript": "^4.9.4"
  }
}
